{
  "location_api": {
    "ip_geolocation": {
      "name": "Ip-location",
      "url": "http://ip-api.com/json/",
      "method": "GET",
      "timeout": 10,
      "params": {
        "fields": "status,lat,lon"
      },
      "response_format": {
        "success_field": "status",
        "success_value": "success",
        "latitude_field": "lat",
        "longitude_field": "lon",
        "error_field": "message"
      }
    }
  },
  "city_api": {
    "xiaomi_location": {
      "name": "City-location",
      "url": "https://weatherapi.market.xiaomi.com/wtr-v3/location/city/geo",
      "method": "GET",
      "timeout": 10,
      "params": {
        "latitude": "{latitude}",
        "longitude": "{longitude}",
        "appKey": "weather20151024",
        "sign": "zUFJoAR2ZVrDy1vF3D07",
        "locale": "{locale}"
      },
      "response_format": {
        "data_path": "0",
        "city_key_field": "key",
        "location_key_field": "locationKey",
        "city_name_field": "name",
        "affiliation_field": "affiliation",
        "latitude_field": "latitude",
        "longitude_field": "longitude"
      }
    }
  },
  "weather_api": {
    "xiaomi_weather": {
      "name": "小米天气",
      "url": "https://weatherapi.market.xiaomi.com/wtr-v3/weather/all?latitude={latitude}&longitude={longitude}&locationKey={location_key}&appKey=weather20151024&sign=zUFJoAR2ZVrDy1vF3D07&isGlobal=false&locale={locale}&days={days}",
      "supports_coordinates": true
    },
    "qweather": {
      "name": "和风天气",
      "url": "https://devapi.qweather.com/v7/weather/now?location={location_key}&key={key}",
      "supports_coordinates": false
    },
    "qweather_air": {
      "name": "和风天气空气质量",
      "url": "https://devapi.qweather.com/v7/air/now?location={location_key}&key={key}",
      "supports_coordinates": false
    },
    "qweather_air_coordinates": {
      "name": "和风天气空气质量(经纬度)",
      "url": "https://devapi.qweather.com/v7/air/now?location={longitude},{latitude}&key={key}",
      "supports_coordinates": true
    },
    "qweather_alert": {
      "name": "和风天气预警",
      "url": "https://devapi.qweather.com/v7/warning/now?location={location_key}&key={key}",
      "supports_coordinates": false
    },
    "qweather_alert_coordinates": {
      "name": "和风天气预警(经纬度)",
      "url": "https://devapi.qweather.com/v7/warning/now?location={longitude},{latitude}&key={key}",
      "supports_coordinates": true
    },
    "amap_weather": {
      "name": "高德天气",
      "url": "https://restapi.amap.com/v3/weather/weatherInfo?key={key}&city={location_key}",
      "supports_coordinates": false
    },
    "qq_weather": {
      "name": "腾讯天气",
      "url": "https://apis.map.qq.com/ws/weather/v1?key={key}&adcode={location_key}",
      "supports_coordinates": false
    },
    "open_meteo": {
      "name": "Open-Meteo天气",
      "url": "https://api.open-meteo.com/v1/forecast?latitude={lat}&longitude={lon}&current=temperature_2m,weather_code,apparent_temperature,relative_humidity_2m,wind_speed_10m,wind_direction_10m,surface_pressure,visibility&hourly=temperature_2m,weather_code,apparent_temperature,relative_humidity_2m,wind_speed_10m,wind_direction_10m,surface_pressure,visibility&daily=temperature_2m_max,temperature_2m_min,weather_code&timezone=auto&forecast_days=7",
      "supports_coordinates": true
    },
    "open_meteo_air_quality": {
      "name": "Open-Meteo空气质量",
      "url": "https://air-quality-api.open-meteo.com/v1/air-quality?latitude={lat}&longitude={lon}&current=carbon_monoxide,nitrogen_dioxide,ozone,pm10,pm2_5,sulphur_dioxide&timezone=auto",
      "supports_coordinates": true
    }
  },
